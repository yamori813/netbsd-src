# The main bus device
mainbus0	at root

# The boot cpu
cpu0		at mainbus?

# STR9100(Orion Family) System-on-Chip
star0		at mainbus?

# NOR Flash
#options	NOR_VERBOSE
options		ARM_CFI_ADDRESS=0x10000000
options		ARM_CFI_BUS_TAG=star_bs_tag
cfi0		at mainbus?
nor0		at cfi0
flash0		at nor0 offset 0x00000000 size 0x00040000 readonly 1 # U-boot

# on-chip 16550 UARTs
com0		at star? addr 0x78000000 irq 10		# UART0

# on-chip Timers
starclk0	at star? addr 0x79000000 irq 0

# on-chip WDT
starwdog0	 at star? addr 0x7a000000 irq 3

# on-chip RTC
starrtc0	at star? addr 0x7b000000 irq 14

# on-chip GPIO
stargpio0	at star? addr 0x7c000000 irq 4	# GPIOA
gpio*		at stargpio?

# 1-Wire support
gpioow* 	at gpio? offset ? mask ?	# 1-wire bitbanging via gpio
gpioow* 	at gpio?
onewire*	at gpioow?

