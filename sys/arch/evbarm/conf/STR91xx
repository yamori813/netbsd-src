file-system 	UMAPFS		# NULLFS + uid and gid remapping
file-system 	UNION		# union file system
#file-system 	CODA		# Coda File System; also needs vcoda (below)
#file-system 	SMBFS		# experimental - CIFS; also needs nsmb (below)
